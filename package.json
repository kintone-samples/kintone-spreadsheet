{
  "name": "kintone_plugin_spreadsheet",
  "version": "1.0.0",
  "main": "index.ts",
  "license": "MIT",
  "scripts": {
    "webpack": "webpack",
    "package": "source .env && echo PPK=$PPK && kintone-plugin-packer src --ppk $PPK",
    "upload": "source .env && kintone-plugin-uploader --domain $DOMAIN --username $USERNAME --password $PASSWORD plugin.zip",
    "build": "yarn webpack && yarn package",
    "watch": "yarn webpack --watch",
    "lint": "yarn eslint src/**/*.tsx src/**/*.ts"
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "always",
    "trailingComma": "all",
    "printWidth": 120
  },
  "dependencies": {
    "@emotion/core": "^10.0.35",
    "@emotion/styled": "^10.0.27",
    "@kintone/kintone-ui-component": "^0.7.2",
    "@kintone/rest-api-client": "^1.4.1",
    "handsontable": "6.2.2",
    "i18next": "^19.4.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.6.0",
    "react-page-visibility": "^6.2.0",
    "react-use": "^15.3.3"
  },
  "devDependencies": {
    "@babel/core": "7.10.2",
    "@kintone/dts-gen": "3.0.4",
    "@kintone/plugin-packer": "3.0.4",
    "@kintone/plugin-uploader": "4.0.4",
    "@types/node": "14.0.13",
    "@types/react": "16.9.38",
    "@types/react-dom": "16.9.8",
    "@types/terser-webpack-plugin": "3.0.0",
    "@types/webpack": "4.41.17",
    "@typescript-eslint/eslint-plugin": "3.3.0",
    "@typescript-eslint/parser": "3.3.0",
    "css-loader": "3.6.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.21.2",
    "eslint-plugin-jest": "23.13.2",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.4",
    "jest": "26.0.1",
    "node-sass": "4.14.1",
    "path": "0.12.7",
    "prettier": "2.0.5",
    "prettier-eslint": "11.0.0",
    "prettier-stylelint": "0.4.2",
    "sass-loader": "8.0.2",
    "style-loader": "1.2.1",
    "terser-webpack-plugin": "3.0.5",
    "ts-loader": "7.0.5",
    "ts-node": "8.10.2",
    "typescript": "3.9.10",
    "webpack": "4.43.0",
    "webpack-bundle-analyzer": "3.8.0",
    "webpack-cli": "3.3.12"
  }
}
